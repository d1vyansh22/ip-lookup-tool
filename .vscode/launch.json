{
    "api": {
        "base_url": "https://ipinfo.io",
        "timeout": 10,
        "max_retries": 3
    },
    "output": {
        "show_emojis": true,
        "show_privacy_info": true,
        "default_format": "formatted"
    },
    "rate_limiting": {
        "delay_between_requests": 0.5,
        "exponential_backoff": true
    }
}
